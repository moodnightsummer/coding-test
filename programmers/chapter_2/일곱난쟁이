function solution(n) {
  let sum = n.reduce((a, b) => a + b) - 100;
  let temp = [];

  outerFor: for (let i = 0; i < n.length; i++) {
    for (let j = 1; j < n.length - 1; j++) {
      if (n[i] + n[j] === sum) {
        temp.push(n[i]);
        temp.push(n[j]);

        break outerFor;
      }
    }
  }

  const answer = n.filter((obj) => !temp.includes(obj));

  return answer;
}

console.log(solution([16, 20, 15, 23, 12, 27, 10, 9, 7]));
